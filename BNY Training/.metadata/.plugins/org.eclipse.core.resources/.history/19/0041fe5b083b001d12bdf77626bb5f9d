package Day1;

public class SequenceOfNo {

//	5.User will enter following sequences 1,2,3,4,6,7,8,11,13,14,15,19,20,21. 
//	You have to generate following o
	
		
		public static void main(String[] args) {

			int a = 3;
			int b = 4;
			int n = 8;

			for (int i = 1; i <= n; i++) {
				int c = a + b;
				System.out.print(a + " " + b + " " + c);
				System.out.println(" ");
				a = c;
				b = b + 1;

			}

		}

	

	
}
